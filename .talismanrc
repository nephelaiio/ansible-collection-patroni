fileignoreconfig:
  - filename: poetry.lock
    ignore_detectors: [filecontent]
  - filename: playbooks/install.yml
    ignore_detectors: [filecontent]
  - filename: playbooks/postgresql.yml
    ignore_detectors: [filename]
  - filename: molecule/install/molecule.yml
    ignore_detectors: [filecontent]
  - filename: molecule/databases/molecule.yml
    ignore_detectors: [filecontent]
  - filename: molecule/barman/molecule.yml
    ignore_detectors: [filecontent]
  - filename: molecule/update/molecule.yml
    ignore_detectors: [filecontent]
  - filename: playbooks/vars/main.yml
    ignore_detectors: [filecontent]
  - filename: playbooks/facts.yml
    ignore_detectors: [filecontent]
  - filename: playbooks/barman.yml
    ignore_detectors: [filecontent]
  - filename: playbooks/patroni.yml
    ignore_detectors: [filecontent]
  - filename: playbooks/templates/patroni.yml.j2
    ignore_detectors: [filecontent]
  - filename: playbooks/templates/postgresql.conf.j2
    ignore_detectors: [filename]
  - filename: playbooks/haproxy.yml
    ignore_detectors: [filecontent]
  - filename: playbooks/update.yml
    ignore_detectors: [filecontent]
  - filename: molecule/common/templates/*
    ignore_detectors: [filecontent]
  - filename: molecule/common/*
    ignore_detectors: [filecontent]
